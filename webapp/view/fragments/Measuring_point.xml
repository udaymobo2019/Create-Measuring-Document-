<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:form="sap.ui.layout.form">
	<Dialog horizontalScrolling="true" icon="sap-icon://functional-location" showHeader="true" state="None" stretch="false"
		title="Technical Object" type="Standard" verticalScrolling="true" contentHeight="" contentWidth="42%">
		<content>
			<IconTabBar expandable="true" expanded="true" applyContentPadding="true" backgroundDesign="Translucent" upperCase="false" visible="true"
				selectedKey="Ok">
				<items>
					<IconTabSeparator icon="" iconDensityAware="false"/>
					<IconTabFilter iconColor="Default" text="By Function Location" count="" design="Vertical" showAll="false" textDirection="Inherit"
						enabled="true" visible="true" iconDensityAware="false">
						<content>
							<HBox>
								<Text text="Functional Location" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"
									class="sapUiTinyMargin"/>
								<ComboBox id="functech" placeholder="" editable="true" change="tech_change1" enabled="true" visible="true" width="100%" valueState="None"
									maxWidth="100%"></ComboBox>
							</HBox>
							<HBox>
								<Text id="sfl" text="Sub Functional Location" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"
									visible="false" class="sapUiTinyMargin"/>
								<ComboBox id="subfunctech" placeholder="" editable="true" change="tech_change" enabled="true" visible="false" width="100%" valueState="None"
									maxWidth="100%"></ComboBox>
							</HBox>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false"/>
					<IconTabFilter iconColor="Default" text="By Equipment" count="" design="Vertical" showAll="false" textDirection="Inherit" enabled="true"
						visible="true" iconDensityAware="false">
						<content>
							<HBox>
								<Text text="Equipment" width="auto" maxLines="1" wrapping="true" textAlign="Begin" textDirection="Inherit" visible="true"
									class="sapUiTinyMargin"/>
								<ComboBox id="equip" placeholder="" editable="true" change="equichange" enabled="true" visible="true" width="100%" valueState="None"
									maxWidth="100%"></ComboBox>
							</HBox>
						</content>
					</IconTabFilter>
				</items>
				<content>
					<Text text="Tab Bar Content goes here ..." width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"
						visible="true"/>
				</content>
			</IconTabBar>
			<Table id="technical_detail" width="auto" noDataText="Select Details" mode="SingleSelect" showSeparators="All" growing="true"
				growingThreshold="20" growingScrollToLoad="true" class="sapUiResponsiveMargin" visible="false">
				<infoToolbar>
					<Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
						<content>
							<Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
						</content>
					</Toolbar>
				</infoToolbar>
				<headerToolbar>
					<OverflowToolbar width="auto" height="auto" design="Solid" visible="true" enabled="true">
						<content>
							<Title text="" titleStyle="Auto" width="auto" textAlign="Begin" visible="true" wrapping="false"/>
							<ToolbarSpacer width=""/>
						</content>
					</OverflowToolbar>
				</headerToolbar>
				<columns>
					<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Phone" demandPopin="false" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="Function Location" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
						</header>
						<footer/>
					</Column>
					<Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="Description" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
						</header>
						<footer/>
					</Column>
				</columns>
				<items>
					<ColumnListItem type="Active">
						<Text id="p1" text=""/>
						<Text id="p2" text=""/>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<beginButton/>
		<buttons>
			<Button text="Clear Filter" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="clearfilter"/>
			<Button id="techok" text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="techok"/>
			<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="canceltecch"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>